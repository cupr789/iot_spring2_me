<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sql_info">

	<select id="selectSql" resultType="map"  parameterType="string">
		${value}
	</select>
	
	<update id="updateSql" parameterType="string">
		${value} 
	</update>
	
	<insert id="insertSql" parameterType="string">
		${value}
	</insert>
	
	<delete id="deleteSql" parameterType="string">
		${value}
	</delete>
	
</mapper>  